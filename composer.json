{
  "name": "theoboldt/juvimg",
  "license": "MIT",
  "type": "project",
  "description": "Application providing image resize service for juvem",
  "authors": [
    {
      "name": "Erik Theoboldt",
      "email": "erik@theoboldt.eu"
    }
  ],
  "require": {
    "php": "^7.2",
    "ext-gd": "*",
    "ext-fileinfo": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "imagine/imagine": "^1.2",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/console": "^4.3",
    "symfony/flex": "^1.0",
    "symfony/framework-bundle": "^4.3",
    "symfony/security-bundle": "^4.3",
    "symfony/yaml": "^4.3"
  },
  "require-dev": {
    "symfony/dotenv": "^4.3"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "bin-dir": "bin",
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
    ],
    "post-update-cmd": [
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C44QAR0T0B83KKE5BW7QBCY9",
      "allow-contrib": false
    }
  }
}
